datasource db {
  provider = "cockroachdb"
  url      = env("DATABASE_URL")
}

generator client {
  provider        = "prisma-client-js"
}

model Dock {
  id        Int      @id 
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  bikes Bike[]
}

model Bike {
  id        Int      @id 
  dockId    Int 
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  dock      Dock   @relation(fields: [dockId], references: [id])
}